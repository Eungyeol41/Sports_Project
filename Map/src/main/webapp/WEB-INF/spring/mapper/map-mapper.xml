<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.team.map.dao.MapDao">

	<select id="selectAll" resultType="MapVO">
		SELECT al_addr FROM tbl_all_list
	</select>
	
	<select id="select" resultType="MapVO">
		SELECT * FROM tbl_map
	</select>
	
	<insert id="insert" parameterType="Map">
		INSERT INTO tbl_map(m_road, m_lat, m_lgt)
		VALUES( #{m_road}, #{m_lat}, #{m_lgt} )
	</insert>
	
</mapper>